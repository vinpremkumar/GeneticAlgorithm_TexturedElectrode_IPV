Pabs_matrix = ( interp(Pabs_x,x+delta_x,y,z,f,x,y,z,f) +
interp(Pabs_y,x,y+delta_y,z,f,x,y,z,f) +
interp(Pabs_z,x,y,z+delta_z,f,x,y,z,f) ) / SP;
