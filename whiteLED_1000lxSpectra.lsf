}
if (havedata("index_act_L","index_act_L_z")) {
Pabs_z = 0.5*eps0*W*(abs(getdata("field_act_L","Ez",1))^2*imag(getdata("index_act_L","index_act_L_z",1)^2));
} else {
Pabs_z = matrix(Nx,Ny,Nz,Nf);
}

